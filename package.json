{
  "name": "more-recipes",
  "version": "1.0.0",
  "engines": {
    "node": "6.11.4"
  },
  "description": "creating a more-recipes site",
  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/babel-cli/bin/babel.js ./server/ -d  dist",
    "migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "unmigrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "heroku-postbuild": "npm run build && npm run migrate",
    "start": "node ./dist/index.js",
    "test": "npm run unmigrate && npm run migrate && mocha --compilers js:babel-core/register server/tests/ --timeout 10000"
  },
  "author": "oluwafemi",
  "license": "ISC",
  "devDependencies": {
    
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.10.0",
    "eslint-config-recommended": "^1.5.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.20.1",
    "sequelize-cli": "^3.0.0",
    "supertest": "^3.0.0"
  }
}

